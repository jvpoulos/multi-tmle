#!/bin/bash
#SBATCH --job-name="static_mpi"
#SBATCH -e static_mpi.err
#SBATCH -p mpi
#SBATCH -n 100
#SBATCH --mincpus=18
#SBATCH --mem-per-cpu=5G
#SBATCH -t 5-00:00:00
#SBATCH --array=1-18

ulimit -l unlimited

module load gcc/6.2.0 R/4.0.1
module load openmpi/4.1.1

#Run the job
mpirun --mca btl tcp,self Rscript tmle_MultinomialTrts.R 'tmle' ${SLURM_ARRAY_TASK_ID} 'binomial' 'TRUE'